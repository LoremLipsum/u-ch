mixin tabs(data)
    +el({
        tag: 'ul',
        class: 'tabs',
        mod: data.mod,
        mix: data.mix,
        js: data.js
    })&attributes(attributes)
        each item in data.items
            +el({
                tag: 'li',
                class: 'tabs__item',
                mod: item.mod,
                mix: item.mix,
                js: item.js
            })
                +el({
                    tag: 'button',
                    class: 'tabs__button',
                    text: item.text
                })
