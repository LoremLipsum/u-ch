mixin button-plus(data)
    +el({
        tag: data.tag ? data.tag : 'button',
        class: 'button-plus',
        mod: data.mod,
        mix: data.mix,
        js: data.js,
        type: data.type,
        text: data.text,
        before: data.before,
        after: data.after,
        target: data.target,
    })&attributes(attributes)
        +el({
            tag: 'span',
            class: 'button-plus__icon',
        })
