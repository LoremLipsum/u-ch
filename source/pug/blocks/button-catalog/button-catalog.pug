mixin button-catalog(data)
    +el({
        tag: data.tag ? data.tag : 'button',
        class: 'button-catalog',
        mod: data.mod,
        mix: data.mix,
        js: data.js,
        type: data.type,
        text: data.text,
        before: data.before,
        after: data.after,
        target: data.target,
    })&attributes(attributes)
